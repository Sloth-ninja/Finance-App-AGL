<script>
// Initialize Lucide icons
lucide.createIcons();

// Global state
let currentTab = 'partners';
let partnersData = [];
let introducersData = [];
let splitsData = [];
let auditData = [];

// Tab switching functionality
function switchTab(tabName) {
    // Hide all tabs
    document.querySelectorAll('.tab-content').forEach(tab => {
        tab.classList.add('hidden');
        tab.classList.remove('opacity-0', 'animate-fade-in-up');
    });
    
    // Remove active class from all tab buttons
    document.querySelectorAll('.tab-btn').forEach(btn => {
        btn.classList.remove('active');
    });
    
    // Show selected tab
    const selectedTab = document.getElementById(tabName + 'Tab');
    const selectedBtn = document.querySelector(`[data-tab="${tabName}"]`);
    
    selectedTab.classList.remove('hidden');
    selectedBtn.classList.add('active');
    
    // Animate tab content
    setTimeout(() => {
        selectedTab.classList.add('opacity-0', 'animate-fade-in-up');
        selectedTab.classList.remove('opacity-0');
    }, 50);
    
    currentTab = tabName;
    
    // Load tab-specific data
    switch(tabName) {
        case 'partners':
            loadPartnersData();
            break;
        case 'introducers':
            loadIntroducersData();
            break;
        case 'splits':
            loadSplitsData();
            break;
        case 'upload':
            loadUploadData();
            break;
        case 'audit':
            loadAuditData();
            break;
    }
}

// Load partners data
function loadPartnersData() {
    // Sample partners data
    partnersData = [
        {
            id: 1,
            name: "John Smith",
            email: "john.smith@finance.com",
            phone: "+44 7700 900123",
            type: "senior",
            defaultSplit: 65,
            status: "active",
            lastUpdated: "2024-01-15",
            initials: "JS"
        },
        {
            id: 2,
            name: "Sarah Johnson",
            email: "sarah.johnson@finance.com",
            phone: "+44 7700 900124",
            type: "associate",
            defaultSplit: 45,
            status: "active",
            lastUpdated: "2024-01-14",
            initials: "SJ"
        }
    ];
    
    renderPartnersTable();
}

// Render partners table
function renderPartnersTable() {
    const tbody = document.getElementById('partnersTableBody');
    tbody.innerHTML = '';
    
    partnersData.forEach(partner => {
        const row = document.createElement('tr');
        row.className = 'data-row';
        row.innerHTML = `
            <td class="px-4 py-4">
                <div class="flex items-center space-x-3">
                    <div class="w-10 h-10 rounded-full bg-gradient-to-br from-blue-500 to-purple-500 flex items-center justify-center text-white font-semibold text-sm">
                        ${partner.initials}
                    </div>
                    <div>
                        <div class="font-semibold text-gray-900">${partner.name}</div>
                        <div class="text-sm text-gray-500">ID: ${partner.id}</div>
                    </div>
                </div>
            </td>
            <td class="px-4 py-4">
                <div class="text-sm text-gray-900">${partner.email}</div>
                <div class="text-sm text-gray-500">${partner.phone}</div>
            </td>
            <td class="px-4 py-4">
                <span class="px-2 py-1 bg-blue-100 text-blue-700 rounded-full text-xs font-medium capitalize">${partner.type}</span>
            </td>
            <td class="px-4 py-4">
                <div class="text-lg font-semibold text-gray-900">${partner.defaultSplit}%</div>
                <div class="text-sm text-gray-500">Default rate</div>
            </td>
            <td class="px-4 py-4">
                <span class="status-${partner.status} px-3 py-1 rounded-full text-xs font-medium capitalize">${partner.status}</span>
            </td>
            <td class="px-4 py-4">
                <div class="text-sm text-gray-900">${partner.lastUpdated}</div>
            </td>
            <td class="px-4 py-4">
                <div class="flex items-center space-x-2">
                    <button class="p-2 text-gray-400 hover:text-indigo-600 hover:bg-indigo-50 rounded-lg transition-colors">
                        <i data-lucide="edit-3" class="w-4 h-4" style="stroke-width: 1.5;"></i>
                    </button>
                    <button class="p-2 text-gray-400 hover:text-green-600 hover:bg-green-50 rounded-lg transition-colors">
                        <i data-lucide="eye" class="w-4 h-4" style="stroke-width: 1.5;"></i>
                    </button>
                </div>
            </td>
        `;
        tbody.appendChild(row);
    });
    
    lucide.createIcons();
}

// Initialize the page
document.addEventListener('DOMContentLoaded', function() {
    // Set initial active tab
    switchTab('partners');
    
    // Add event listeners for animations
    setTimeout(() => {
        document.querySelector('aside').classList.remove('opacity-0');
        document.querySelector('header').classList.remove('opacity-0');
        document.querySelector('.opacity-0.animate-fade-in-up.delay-200').classList.remove('opacity-0');
    }, 200);
});

